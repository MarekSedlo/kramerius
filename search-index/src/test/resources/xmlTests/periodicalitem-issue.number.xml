            <field name="n.date_range_end.month">6</field>
            <field name="n.date_range_end.year">1925</field>
            <field name="n.date_range_start.day">6</field>
            <field name="n.date_range_start.month">6</field>
            <field name="n.date_range_start.year">1925</field>

            <field name="n.part.number.sort">23</field>
            <field name="n.part.number.str">23</field>

            <field name="n.title.search">Deutsche Nachrichten. 23</field>
            <field name="n.title.sort">DEUTSH|E NAH|RIH|TEN 23</field>
            <field name="n.titles.search">Deutsche Nachrichten. 23</field>
