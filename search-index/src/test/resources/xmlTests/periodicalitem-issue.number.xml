    <test name="issue number je v titleInfo i v part, titleInfo ma vetsi vahu">
        <desc>uuid:34c79cc8-58b4-4776-b254-adb609270e6b</desc>
        <in>
            <titleInfo>
                <title>Deutsche Nachrichten</title>
                <partNumber>23</partNumber>
            </titleInfo>
            <originInfo>
                <dateIssued>6. 6. 1925</dateIssued>
            </originInfo>
            <part type="issue">
                <detail>
                    <number>24</number>
                </detail>
                <date>6. 6. 1925</date>
            </part>
        </in>
        <out>
            <field name="n.date.max">1925-06-06T23:59:59.999Z</field>
            <field name="n.date.min">1925-06-06T00:00:00.001Z</field>
            <field name="n.date.str">6. 6. 1925</field>
            <!--<field name="n.date_instant.day">6</field>
            <field name="n.date_instant.month">6</field>
            <field name="n.date_instant.year">1925</field>-->
